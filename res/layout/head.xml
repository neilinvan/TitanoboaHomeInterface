<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/backgroundmodule"
    android:gravity="top" >

<ProgressBar
    android:id="@+id/progressBarmotor"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignParentLeft="true"
    android:layout_alignParentTop="true"
    android:layout_marginLeft="80dp"
    android:layout_marginTop="134dp" />

<ProgressBar
    android:id="@+id/progressBarpressure"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_alignLeft="@+id/progressBarmotor"
    android:layout_below="@+id/progressBarmotor"
    android:layout_marginTop="25dp" />

<EditText
    android:id="@+id/editText1"
    android:layout_width="60dp"
    android:layout_height="28dp"
    android:layout_marginTop="150dp"
    android:layout_marginLeft="36dp"
    android:layout_toRightOf="@+id/progressBarmotor"
    android:ems="10"
    android:inputType="number" >

    <requestFocus />
</EditText>

<AutoCompleteTextView
    android:id="@+id/autoCompleteTextView1"
    android:layout_width="60dp"
    android:layout_height="28dp"
    android:layout_alignLeft="@+id/editText1"
    android:layout_alignRight="@+id/editText1"
    android:layout_alignTop="@+id/progressBarpressure"
    android:ems="10"
    android:text="AutoCompleteTextView" />

</RelativeLayout>